 <Method>
  <ItemName>DrawInContext</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub DrawInContext(viaScripter as KarelScripter)</SourceLine>
   <SourceLine>dim frameWidth as integer = viaScripter.Width</SourceLine>
   <SourceLine>dim frameHeight as integer = viaScripter.Height</SourceLine>
   <SourceLine>const insetFrame = 2</SourceLine>
   <SourceLine>viaScripter.PenSize = 4</SourceLine>
   <SourceLine>viaScripter.DrawRect insetFrame, insetFrame, frameWidth-insetFrame, frameHeight-insetFrame</SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>0</ItemFlags>
  <Shared>0</Shared>
  <ItemParams>viaScripter as KarelScripter</ItemParams>
  <ItemResult></ItemResult>
 </Method>
